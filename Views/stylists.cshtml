<h2>Stylists:</h2>
<ul>
  @if (Model.Count != 0)
  {
    @foreach (var stylist in Model)
    {
      <li><a href="/stylists/@stylist.GetId()">@stylist.GetName()</a>
       | <a href="/stylist/edit/@stylist.GetId()">Edit</a>
       | <a href="stylist/delete/@stylist.GetId()">Delete</a>
      </li>
    }
  }
</ul>
<h4><a href="/stylists/new">Add new stylist</a></h4>
<h4><a href="/">Return home</a></h4>
